{
  "name": "hn-pwa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "standard",
    "lint:css": "sass-lint -v -q",
    "lint:fix": "standard --fix",
    "build:css": "node-sass src/client/styles/pages -o build/public/css --output-style compressed",
    "build:app": "webpack",
    "build:server": "babel src -d lib --ignore src/client",
    "watch:css": "npm run build:css && node-sass src/client/styles/pages -wro build/public/css;",
    "watch:app": "webpack --watch",
    "watch:server": "nodemon --watch src --ignore src/client --exec 'npm run build:server && node ./lib/index.js'",
    "prebuild": "rm -rf ./build ./lib",
    "prewatch": "npm run prebuild",
    "build": "NODE_ENV=production npm run build:server && npm run build:app && npm run build:css",
    "watch": "npm run watch:css & npm run watch:app & npm run watch:server",
    "start": "node ./lib/index.js",
    "deploy": "now --public",
    "debug": "npm run build && node --inspect ./lib/index.js",
    "lighthouse-visual": "lighthouse http://localhost:3000/ --view",
    "lighthouse": "npm run build && node ./scripts/lighthouse.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "lighthouse": "^2.9.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.1",
    "sass-lint": "^1.12.1",
    "standard": "^11.0.0",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "ecstatic": "^3.0.0",
    "express": "^4.15.4",
    "history": "^4.7.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
