{
  "name": "hn-pwa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:app": "webpack",
    "build:server": "babel src -d lib --ignore src/client",
    "watch:app": "webpack --watch",
    "watch:server": "nodemon --watch src --ignore src/client --exec 'npm run build:server && node ./lib/index.js'",
    "prebuild": "rm -rf ./build ./lib",
    "prewatch": "npm run prebuild",
    "build": "NODE_ENV=production npm run build:server && npm run build:app",
    "watch": "npm run watch:app & npm run watch:server",
    "start": "node ./lib/index.js",
    "debug": "npm run build && node --inspect ./lib/index.js",
    "lighthouse-visual": "lighthouse http://localhost:3000/ --view",
    "lighthouse": "npm run build && node ./scripts/lighthouse.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "lighthouse": "^2.9.1",
    "nodemon": "^1.17.1",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "ecstatic": "^3.0.0",
    "express": "^4.15.4",
    "history": "^4.7.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "^3.7.2"
  }
}
